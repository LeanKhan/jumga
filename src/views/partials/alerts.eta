<!-- TODO: show other alert types -->
<% if (alerts.error.length || alerts.success.length) { %>
<ul>
  <% alerts.error.forEach(alert => { %>
  <li>
    <div class="notification floating is-danger">
      <button class="delete"></button>
      <%= alert.msg %>
    </div>
  </li>
  <% }) %> <% alerts.success.forEach(alert => { %>
  <li>
    <div class="notification floating is-success">
      <button class="delete"></button>
      <%= alert.msg %>
    </div>
  </li>
  <% }) %>
</ul>
<% } %>
